.swiper {
  height: 400px;
  width: 100%;
  display: flex;
  flex-direction: column-reverse;

  .swiper-slide {
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
    font-weight: bold;
    font-size: 12px;
  }
}

.swiper-menu {
  position: relative;
  display: flex;
}

